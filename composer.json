{
  "name": "raythompweb/clashvibes-plugin",
  "autoload": {
    "psr-4": {
      "Raythompweb\\ClashvibesPlugin\\": "src/"
    }
  },
  "authors": [
    {
      "name": "raythompsonwebdev",
      "email": "raymond.thompson@raythompsonwebdev.co.uk"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "wptrt/wpthemereview": "^0.2.1",
    "php-parallel-lint/php-parallel-lint": "^1.2.0"
  },
  "scripts": {
    "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
    "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
    "make-pot": "wp i18n make-pot . languages/clashvibes.pot"
  },
  "support": {
    "issues": "https://github.com/Automattic/clashvibes/issues",
    "source": "https://github.com/Automattic/clashvibes"
  },
  "config": {
    "optimize-autoloader": true
  }
}
